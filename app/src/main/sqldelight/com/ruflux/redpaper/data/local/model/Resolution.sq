import java.lang.Integer;

CREATE TABLE resolution (
    post_id TEXT NOT NULL,
    url TEXT NOT NULL,
    width INTEGER As Integer NOT NULL,
    height INTEGER As Integer NOT NULL,

    FOREIGN KEY (post_id) REFERENCES post(post_id) ON DELETE CASCADE
);

SelectAllResolutionByPost:
SELECT *
FROM resolution
JOIN post ON post.post_id = resolution.post_id
WHERE post.post_id = ?;

InsertResolution:
INSERT INTO resolution VALUES (?,?,?,?);